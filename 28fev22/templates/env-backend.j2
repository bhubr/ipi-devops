# The port you want the server to listen to
PORT={{ port }}

# DB settings
DB_NAME={{ dbname }}
DB_USER={{ dbuser }}
DB_PASS={{ dbpass }}

# JWT secret
JWT_SECRET={{ jwt_secret }}

# Authorized client app origin (for CORS)
CLIENT_APP_ORIGIN={{ client_app_origin }}

# Built docs path - must have been built with Slate
# Leave empty for no docs
# DOCS_ROOT=
